<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <corners android:radius="5dp"/>
            <solid android:color="@color/buttonEnable"/>
            <stroke android:width="3dp" android:color="@color/buttonEnableBorder"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="5dp"/>
            <solid android:color="@color/buttonDisable"/>
            <stroke android:width="3dp" android:color="@color/buttonDisableBorder"/>
        </shape>
    </item>
</selector>